<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <sheetTitle>LLL:EXT:media2click/Resources/Private/Language/locallang_db.xlf:flexform.plugin.title</sheetTitle>
                <type>array</type>
                <el>
                    <settings.includeHosts>
                        <label>LLL:EXT:media2click/Resources/Private/Language/locallang_db.xlf:flexform.plugin.includeHosts</label>
                        <config>
                            <type>select</type>
                            <renderType>selectMultipleSideBySide</renderType>
                            <allowed>tx_media2click_domain_model_host</allowed>
                            <foreign_table>tx_media2click_domain_model_host</foreign_table>
                            <foreign_table_where>AND {#tx_media2click_domain_model_host}.{#allow_permanent} = 1 AND
                                {#tx_media2click_domain_model_host}.{#sys_language_uid}
                                IN(###REC_FIELD_sys_language_uid###, -1)
                            </foreign_table_where>
                            <minitems>0</minitems>
                            <maxitems>999</maxitems>
                            <size>5</size>
                            <autoSizeMax>15</autoSizeMax>
                            <disableNoMatchingValueElement>1</disableNoMatchingValueElement>
                            <enableMultiSelectFilterTextfield>1</enableMultiSelectFilterTextfield>
                        </config>
                    </settings.includeHosts>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
